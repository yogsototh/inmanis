function log(msg) {
    if (console && console.log) {
        console.log(msg);
    }
}
log("I'm here");

function clicked(prefix) {
    return function() {
                var parentId = $(this).parent().attr('id');
                log(prefix + parentId); 
                };
}

$(document).ready(function(){
    $(".yeah").click(clicked("Yeah! for "));
    $(".neah").click(clicked("Neah! for "));
        
});
